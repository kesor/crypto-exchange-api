{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "extends": [
    "plugin:flowtype/recommended",
    "eslint:recommended",
    "standard"
  ],
  "plugins": [
    "flowtype",
    "standard"
  ],
  "rules": {
    "complexity": ["error", { "max": 5 }]
  }
}